{
    "name": "testbed15-viz",
    "datasets": {
        "testbed15": {
            "type": "thelper.data.geo.parsers.VectorCropDataset",
            "params": {
                "raster_path": "data/testbed15/*.tif",
                "vector_path": "data/testbed15/hydro_original.geojson",
                "srs_target": "3857",
                "vector_target_prop": {"TYPECE": "21"},
                "vector_roi_buffer": 300,
                "reproj_rasters": true
            }
        }
    },
    "loaders": {
        "workers": 0,
        "collate_fn": {
            "type": "thelper.data.loaders.default_collate",
            "params": {"force_tensor": false}
        },
        "train_split": {
            "testbed15": 1.0
        }
    },
    "viz": {
        "kwargs": {
            "block": true,
            "ch_transpose": false
        }
    }
}
