{
    "name": "testbed15-viz",
    "datasets": {
        "testbed15": {
            "type": "thelper.data.geo.ogc.TB15D104Dataset",
            "params": {
                "raster_path": "data/testbed15/*.tif",
                "vector_path": "data/testbed15/hydro_original.geojson",
                "px_size": 3,
                "lake_area_min": 100,
                "lake_area_max": 200000,
                "lake_river_max_dist": 300,
                "roi_buffer": 1000,
                "srs_target": "3857",
                "reproj_rasters": true,
                "display_debug": true
            }
        }
    },
    "loaders": {
        "workers": 0,
        "collate_fn": {
            "type": "thelper.data.loaders.default_collate",
            "params": {"force_tensor": false}
        },
        "train_split": {
            "testbed15": 1.0
        }
    },
    "viz": {
        "kwargs": {
            "block": true,
            "ch_transpose": false
        }
    }
}
